#include <stdio.h>
//Rajdeep Singnh Sidhu
int main(void) {
	// your code goes here
	int t;scanf("%d", &t);
	for(int i=0; i<t;i++)
	{
	    int n; scanf("%d", &n);
	    if(n>=3)
	    {
    	    if(n%3==0)
    	    {
    	        printf("YES\n");
    	    }
    	    else{
    	        printf("NO\n");
    	    }
	    }
	    else{
	        printf("NO\n");
	    }
	}
	return 0;
}

