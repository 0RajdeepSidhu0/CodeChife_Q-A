#include <stdio.h>
//Author: Rajdeep Singh Sidhu
//Author: 10-07-2023
int main(void) {

	int T;
	scanf("%d",&T);
	for(int i=0; i<T; i++)
	{
	    int X,Y,A;
	    scanf("%d%d%d",&X,&Y,&A);
	    if(A>=X && A<Y)
	    {
	        printf("YES\n");
	    }
	    else
	    {
	        printf("NO\n");
	    }
	}
	return 0;
}

