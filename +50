#include <stdio.h>

int main(void) {
	// your code goes here
	int T;
	scanf("%d",&T);
	for(int i=0; i<T; i++)
	{
	    int X,Y,A;
	    scanf("%d%d%d",&X,&Y,&A);
	    if(A>=X)
	    {
	        if(A<Y)
	        {
	            printf("YES\n");
	        }
	        else
	        {
	            printf("NO\n");
	        }
	    }
	    else
	    {
	        printf("NO\n");
	    }
	}
	return 0;
}

