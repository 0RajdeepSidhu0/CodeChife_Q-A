#include <stdio.h>
//Author: Rajdeep Singh Sidhu
//Date: 30-05-2023
int main(void) {
	// your code goes here
	int t;
	scanf("%d", &t);
	for(int i=0; i<t; i++)
	{
	    int x,y;scanf("%d%d", &x, &y);
	    if(y>x)
	    {
	        y-=x;
	        printf("%d\n", y);
	    }
	    else
	    {
	        printf("%d", 0);
	    }
	}
	return 0;
}

