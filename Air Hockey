#include <stdio.h>

int main(void) {
	// your code goes here
	int t;scanf("%d", &t);
	for(int i=0;i<t;i++)
	{
	    int x,y;scanf("%d%d",&x,&y);
	    if(x>y)
	    {
	        x=7-x;
	        printf("%d\n", x);
	    }
	    else if(x==y)
	    {
	        y=7-y;
	        printf("%d\n", y);
	    }
	    else{
	        y=7-y;
	        printf("%d\n", y);
	    }
	}
	return 0;
}

